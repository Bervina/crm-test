{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "react-app",
        "prettier",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended"
    ],

    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier", "react-hooks"],

    "rules": {
        "prettier/prettier": ["error", {}, { "usePrettierrc": true }]

    }
}
